{
    "compilerOptions": {
        "target": "es2018",
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "out",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true
      
    }
}